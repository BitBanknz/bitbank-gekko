{
  "name": "bitbank-gekko",
  "version": "1.0.0",
  "description": "bitbank gekko plugin",
  "main": "index.js",
  "scripts": {
    "build": "npm run tsc && npm run browserify && npm run copyfiles",
    "copyfiles": "copyfiles -u 2 src/strategies/*.toml dist",
    "tsc": "tsc",
    "browserify": "npm run browserify-1 && npm run browserify-2 && npm run browserify-3",
    "browserify-1": "browserify tmp/strategies/BitBankStrategy.js -o dist/BitBankStrategy.js --no-commondirs --node -s BitBankStrategy",
    "browserify-2": "browserify tmp/strategies/BitBankStrategy2.js -o dist/BitBankStrategy2.js --no-commondirs --node -s BitBankStrategy2",
    "browserify-3": "browserify tmp/strategies/BitBankStrategyOnlyBacktestingPreprocessedData.js -o dist/BitBankStrategyOnlyBacktestingPreprocessedData.js --no-commondirs --node -s BitBankStrategyOnlyBacktestingPreprocessedData",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/es6-shim": "^0.31.35",
    "@types/mocha": "^2.2.48",
    "@types/moment": "^2.13.0",
    "@types/node": "^9.4.5",
    "awesome-typescript-loader": "^3.4.1",
    "browserify": "^16.1.0",
    "chai": "^4.1.2",
    "copyfiles": "^1.2.0",
    "mocha": "^5.0.0",
    "source-map": "^0.7.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "@types/es6-promise": "^3.3.0",
    "bitbank-node-js-api": "^1.0.0",
    "flatten-prototypes": "^4.0.0",
    "fs": "0.0.1-security",
    "line-by-line": "^0.1.6",
    "moment": "^2.20.1"
  }
}
